<?xml version="1.0" encoding="UTF-8"?><odoo>    <record id="view_stock_landed_cost_form" model="ir.ui.view">        <field name="name">view.stock.landed.cost.form.inherit.import.manex</field>        <field name="model">stock.landed.cost</field>        <field name="inherit_id" ref="stock_landed_costs.view_stock_landed_cost_form"/>        <field name="arch" type="xml">            <button name="button_validate" position="attributes">                <attribute name="groups">import_manex.group_stock_landed_import</attribute>            </button>            <button name="button_validate" position="after">                <button name="buscar_movimientos"                        string="Buscar movimientos"                        type="object"                        class="btn-primary"/>            </button>            <field name="picking_ids" position="before">                <field name="purchase_ids" widget="many2many_tags" options="{'no_create': True}"                       domain="[('stock_landed_cost_id', '=', False)]"/>            </field>            <field name="vendor_bill_id" position="after">                <field name="ref"/>            </field>            <xpath expr="//div[@name='button_box']" position="inside">                <button name="action_print_report"                        type="object"                        class="oe_stat_button"                        icon="fa-print">                    <div class="o_stat_info">                        <span class="o_stat_text">Imprimir</span>                    </div>                </button>            </xpath>            <xpath expr="//page[@name='valuation_adjustments']" position="after">                <page name="Apuntes Contables">                    <field name="move_line_ids" readonly="1">                        <list>                            <field name="move_id"/>                            <field name="account_id"/>                            <field name="partner_id"/>                            <field name="name"/>                            <field name="date"/>                            <field name="balance"/>                        </list>                    </field>                </page>                <page name="Asientos Contables">                    <field name="move_ids" domain="[('move_type', 'in',  ['in_invoice', 'in_refund', 'in_receipt', 'entry']),                    ('use_landed_cost', '=',  False)]"/>                </page>            </xpath>        </field>    </record></odoo>